{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"linter": {
		"rules": {
			"all": true,
			"suspicious": {
				"noConsole": "off",
				"noConsoleLog": "off"
			},
			"style": {
				"noDefaultExport": "off"
			}
		}
	},
	"files": {
		"ignoreUnknown": true
	},
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true,
		"defaultBranch": "main"
	}
}
